{
    "editor.fontFamily": "'Monaco','Droid Sans Mono', 'monospace', monospace, 'Droid Sans Fallback'",
    "editor.fontSize": 15,
    "terminal.integrated.fontFamily": "Hack",
    "terminal.integrated.fontSize": 15,
    "editor.autoIndent": "full",
    "editor.formatOnPaste": true,
    "editor.formatOnType": true,
    "files.associations": {
        "iostream": "cpp",
        "bitset": "cpp",
        "random": "cpp",
        "ostream": "cpp",
        "string": "cpp"
    },
    
    "editor.suggest.showVariables": true,
    "editor.suggest.showKeywords": true,
    "editor.quickSuggestions": true,
    "editor.suggest.showClasses": true,
    "cph.defaultLanguage": "cpp",
    "cph.saveLocation": "/home/taalpatar_shepai/Documents/Extra Files",
    "editor.wordWrap": "on",
    "editor.minimap.enabled": false,
    "editor.suggest.showFunctions": true,
    "editor.suggest.showFiles": true,
    "editor.suggest.showReferences": true,
    "editor.suggest.showFolders": true,
    "editor.suggest.showOperators": true,
    "editor.suggest.showTypeParameters": true,
    "editor.quickSuggestionsDelay": 15,
    "cph.argsCpp": "-Wmaybe-uninitialized -std=c++17",
    "C_Cpp.default.cppStandard": "c++20",
    "C_Cpp.default.cStandard": "c18",
    "editor.suggest.localityBonus": true,
    "editor.suggest.shareSuggestSelections": true,
    "editor.suggest.statusBar.visible": true,
    "telemetry.enableCrashReporter": false,
    "telemetry.enableTelemetry": false
}